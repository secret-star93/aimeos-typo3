{
	"name": "aimeos/typo3-extension",
	"description": "Aimeos TYPO3 extension for web shops, portals and market places",
	"homepage": "http://www.aimeos.org",
	"license": "LGPL-3.0",
	"keywords": ["e-commerce", "shop", "portal", "market place"],
	"support": {
		"source": "https://github.com/aimeos/typo3-extension",
		"issues": "https://github.com/aimeos/typo3-extension/issues",
		"forum": "http://help.aimeos.org",
		"wiki": "http://docs.aimeos.org"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/nsendetzky/arcavias-core"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"arcavias/arcavias-core": "dev-master",
		"aimeos/extinstaller": "dev-master",
		"aimeos/ai-typo3": "dev-master",
		"aimeos/ai-zend2": "dev-master"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "1.*",
		"phpunit/phpunit": "3.7.*",
		"phing/phing": "2.4.*"
	},
	"extra": {
		"ext-path": "Resources/Public/ext/"
	}
}
